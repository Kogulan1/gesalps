{
  "functions": {
    "api/*.py": {
      "runtime": "python3.11",
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/v1/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/dev/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/docs",
      "dest": "/api/main.py"
    },
    {
      "src": "/openapi.json",
      "dest": "/api/main.py"
    }
  ]
}
